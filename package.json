{
  "name": "oboe",
  "title": "Oboe.js",
  "version": "2.1.3",
  "description": "Oboe.js reads json, giving you the objects as they are found without waiting for the stream to finish",
  "main": "./dist/oboe-node.js",
  "browser": "./dist/oboe-browser.js",
  "scripts": {
    "test": "grunt test",
    "test-start-server": "grunt test-start-server",
    "test-run": "grunt test-run",
    "test-node": "grunt test-node",
    "test-browser": "grunt test-unit",
    "test-concat": "grunt test-concat",
    "test-http": "grunt test-http",
    "test-minified": "grunt test-minified",
    "test-amd": "grunt test-amd",
    "jasmine-node": "jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "browser-test-auto-run": "grunt test-auto-run",
    "node-test-auto-run": "grunt node-test-auto-run",
    "dist-sizes": "grunt dist-sizes",
    "clean-dist": "rm dist/*.js && rm build/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jimhigson/oboe.js.git"
  },
  "keywords": [
    "json",
    "parser",
    "stream",
    "progressive",
    "http",
    "sax",
    "event",
    "emitter",
    "async",
    "browser"
  ],
  "homepage": "http://oboejs.com",
  "author": "Jim Higson",
  "license": "BSD",
  "readmeFilename": "README.md",
  "devDependencies": {
    "color": "~0.4.4",
    "cors": "~2.1.1",
    "doctoc": "~0.4.3",
    "express": "~3.4.3",
    "get-json": "0.0.1",
    "grunt": "~0.4.1",
    "grunt-cli": "~0.1.9",
    "grunt-concurrent": "~0.3.1",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-concat": "~0.1.3",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-contrib-watch": "~0.5.1",
    "grunt-exec": "~0.4.2",
    "grunt-express-server": "0.5.3",
    "grunt-karma": "2.0.0",
    "grunt-micro": "~0.1.0",
    "grunt-wrap": "~0.2.0",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "jasmine-node": "~1.11.0",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-safari-launcher": "1.0.0",
    "matchdep": "~0.1.2",
    "request": "^2.55.0",
    "sinon": "=1.17.3"
  },
  "dependencies": {
    "http-https": "^1.0.0"
  },
  "jam": {
    "main": "dist/oboe-browser.js",
    "include": [
      "dist/oboe-browser.js",
      "LICENCE",
      "package.json",
      "README.md"
    ],
    "dependencies": {},
    "categories": [
      "AJAX & Websockets",
      "Parsers & Compilers"
    ]
  }
}
